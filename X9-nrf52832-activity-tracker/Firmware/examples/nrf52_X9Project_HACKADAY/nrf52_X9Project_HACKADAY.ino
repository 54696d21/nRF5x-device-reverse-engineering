    unsigned long millisPerTransmit = 1000; // 1HZ

    //using Bluetooth serial for debugging can be a little trickier than traditional USB UART serial connections
    bleSerial.poll();
    
    delay(100); //time to manually connect for serial debugging before rest of setup
    bleSerial.println("Setup");
    delay(50);
  
 //   ssd1306_setFixedFont(ssd1306xled_font6x8);

    ssd1331_96x64_spi_init(OLED_RES, OLED_CS, OLED_DC);  

    //start at the top
    placeHolder = 0;

}

